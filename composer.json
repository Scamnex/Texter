{
  "name": "fuyutsuki/texter",
  "description": "This is a PocketMine-MP plugin that can add, edit, move and remove FloatingTextParticle.",
  "type": "project",
  "homepage": "https://github.com/fuyutsuki/Texter",
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-hash": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcre": "*",
    "ext-phar": "*",
    "ext-pthreads": ">=3.1.7dev",
    "ext-reflection": "*",
    "ext-sockets": "*",
    "ext-spl": "*",
    "ext-yaml": ">=2.0.0",
    "ext-zip": "*",
    "ext-zlib": ">=1.2.11",
    "pocketmine/pocketmine-mp": "dev-master as 3.2.3",
    "pocketmine/raklib": "^0.12.0",
    "pocketmine/spl": "^0.3.0",
    "pocketmine/binaryutils": "^0.1.0",
    "pocketmine/nbt": "^0.2.0",
    "pocketmine/math": "dev-master as 0.2.1",
    "pocketmine/snooze": "^0.1.0",
    "fuyutsuki/libform": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "": ["src\\"]
